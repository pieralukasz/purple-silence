{
  "name": "purple-silence-mobile",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "link": "react-native link",
    "test": "jest --silent --testEnvironment jsdom --timers fake --coverage",
    "generate": "npx codeandpepper/generate component --type=Mobile",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "format-check": "prettier --check \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint --ext .ts,.tsx src --color",
    "types-check": "npx tsc --project . --noEmit --pretty --skipLibCheck",
    "clear": "watchman watch-del-all && yarn clear:android && yarn clear:ios && rm -rf yarn.lock node_modules && yarn install",
    "clear:android": "cd android && ./gradlew clean",
    "clear:ios": "cd ios && rm -rf pods Podfile.lock && pod install",
    "dt:lint": "eslint --ext .ts ./detox --color",
    "dt:format": "prettier --write \"detox/**/*.{ts,js,json}\"",
    "dt:build:ios": "detox build --configuration ios",
    "dt:build:android": "detox build --configuration android",
    "dt:run:ios": "detox test ./detox --configuration ios --reuse",
    "dt:run:android": "detox test ./detox --configuration android",
    "postinstall": "cd .. && husky install mobile/.husky && cd mobile && yarn format"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@aws-amplify/core": "^4.1.1",
    "@hookform/resolvers": "^2.6.0",
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "amazon-cognito-identity-js": "^5.0.3",
    "aws-amplify": "^4.1.1",
    "aws-appsync-auth-link": "^3.0.4",
    "aws-appsync-subscription-link": "^3.0.6",
    "expo-secure-store": "^10.1.0",
    "google-libphonenumber": "^3.2.21",
    "i18next": "^20.3.3",
    "i18next-locize-backend": "^4.2.3",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-hook-form": "^7.5.3",
    "react-i18next": "^11.11.3",
    "react-native": "0.64.1",
    "react-native-device-info": "^8.1.3",
    "react-native-dotenv": "^3.1.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "^2.2.0",
    "react-native-restart": "^0.0.19",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-svg": "^12.1.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@oktadev/schematics": "3.4.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/detox": "^17.14.1",
    "@types/faker": "^5.5.7",
    "@types/google-libphonenumber": "^7.4.21",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.64.5",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-restart": "^0.0.14",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^18.18.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.64.0",
    "mocha": "^9.0.2",
    "prettier": "^2.3.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "detox"
    ],
    "resetMocks": false,
    "moduleNameMapper": {
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@consts/(.*)": "<rootDir>/src/consts/$1",
      "@enums/(.*)": "<rootDir>/src/types/enums/$1",
      "@hooks/(.*)": "<rootDir>/src/hooks/$1",
      "@features/(.*)": "<rootDir>/src/features/$1",
      "@i18n/(.*)": "<rootDir>/src/i18n/$1",
      "@interfaces/(.*)": "<rootDir>/src/types/interfaces/$1",
      "@layouts/(.*)": "<rootDir>/src/layouts/$1",
      "@screens": "<rootDir>/src/screens/$1",
      "@themes/(.*)": "<rootDir>/src/themes/$1",
      "@utils/(.*)": "<rootDir>/src/utils/$1",
      "\\.svg": "<rootDir>/src/__mocks__/svgrMock.ts"
    },
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/index.ts",
      "!src/**/*Params.ts",
      "!src/**/*State.ts",
      "!src/types/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    }
  }
}
