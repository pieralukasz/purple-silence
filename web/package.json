    "react-scripts": "4.0.3",
    "react-waypoint": "^10.1.0",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.10",
    "@cypress/instrument-cra": "^1.4.0",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/near-operation-file-preset": "^1.18.0",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@percy/cli": "^1.0.0-beta.63",
    "@percy/cypress": "^3.1.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/faker": "^5.5.5",
    "@types/google-libphonenumber": "^7.4.21",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/mochawesome": "^6.2.0",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/retry": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "cypress": "^8.3.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^1.5.0",
    "cypress-xpath": "^1.6.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "mocha": "^9.0.1",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "^2.3.2",
    "react-app-rewired": "^2.1.8",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "generate-graphql": "graphql-codegen --config codegen.yml && yarn format",
    "analyze-bundle": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-app-rewired test",
    "test:nowatch": "react-app-rewired test --watchAll=false --coverage",
    "lint": "eslint --ext .ts,.tsx ./src --color",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "eject": "react-scripts eject",
    "cy:lint": "eslint --ext .ts ./cypress --color",
    "cy:format": "prettier --ignore-path ./cypress/.prettierignore --write \"./cypress/**/*.{ts,json}\"",
    "cy:open": "cypress open --config-file ./cypress/cypress.json",
    "cy:run": "percy exec -c ./cypress/.percy.json -- cypress run --config-file ./cypress/cypress.json -b chrome",
    "cy:coverage:start": "react-app-rewired -r @cypress/instrument-cra start",
    "cy:mocha:clean": "rm -rf ./cypress/reports/mochawesome || true",
    "cy:mocha:combine": "mochawesome-merge ./cypress/reports/mochawesome/single-specs/*.json -o ./cypress/reports/mochawesome/final-report/summary.json",
    "cy:mocha:generate": "marge -o ./cypress/reports/mochawesome/final-report ./cypress/reports/mochawesome/final-report/summary.json",
    "cy:mocha:generate:report": "yarn cy:mocha:combine && yarn cy:mocha:generate",
    "postinstall": "cd .. && husky install web/.husky && cd web && yarn format"
  },
  "nyc": {
    "report-dir": "./cypress/reports/code-coverage",
    "temp-dir": "./cypress/reports/code-coverage/.nyc_output",
    "reporter": [
      "html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": false,
    "moduleNameMapper": {
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@consts/(.*)": "<rootDir>/src/consts/$1",
      "@enums/(.*)": "<rootDir>/src/types/enums/$1",
      "@features/(.*)": "<rootDir>/src/features/$1",
      "@generated/(.*)": "<rootDir>/src/types/generated/$1",
      "@i18n/(.*)": "<rootDir>/src/i18n/$1",
      "@interfaces/(.*)": "<rootDir>/src/types/interfaces/$1",
      "@layouts/(.*)": "<rootDir>/src/layouts/$1",
      "@mocks/(.*)": "<rootDir>/src/mocks/$1",
      "@routing/(.*)": "<rootDir>/src/routing/$1",
      "@themes/(.*)": "<rootDir>/src/themes/$1",
      "@utils/(.*)": "<rootDir>/src/utils/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
